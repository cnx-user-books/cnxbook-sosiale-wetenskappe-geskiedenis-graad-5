        <figure id="id1170255069941">
          <media id="id1170255069941_media" alt="">
            <image mime-type="image/png" src="../../media/graphics5-d6f3.png" id="id1170255069941__onlineimage" height="232" width="617"/>
          </media>
        </figure>
