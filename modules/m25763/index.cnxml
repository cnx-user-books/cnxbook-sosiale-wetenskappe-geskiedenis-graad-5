            <tbody>
              <row>
                <entry/>
                <entry/>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry/>
                <entry/>
                <entry>WAAR</entry>
                <entry>VALS</entry>
              </row>
              <row>
                <entry>1.</entry>
                <entry>Terwyl almal dans, sit die vrouens eenkant en klap hande.</entry>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>2.</entry>
                <entry>Die mans dra drie stokke om maat of ritme mee te hou.</entry>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>3.</entry>
                <entry>Die mans dra musse met wildsbokhare.</entry>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>4.</entry>
                <entry>Die tekening was oorspronklik in rooi.</entry>
                <entry/>
                <entry/>
              </row>
