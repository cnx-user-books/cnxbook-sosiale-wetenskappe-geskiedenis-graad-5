                <entry>VALS</entry>
              </row>
              <row>
                <entry>5.</entry>
                <entry>Die San het meestal alleen gejag.</entry>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>6.</entry>
                <entry>Hul kon diere jag wat groter en sterker as hulle was.</entry>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry>7.</entry>
                <entry>Die San was goeie jagters.</entry>
                <entry/>
                <entry/>
              </row>
              <row>
                <entry/>
                <entry/>
                <entry/>
                <entry/>
              </row>
            </tbody>
          </tgroup>
